{
	"name": "@konecty/sdk",
	"version": "1.0.0",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"author": "Derotino Silveira <silveirado@gmail.com>",
	"license": "MIT",
	"files": [
		"/dist"
	],
	"exports": {
		"./Client": "./dist/sdk/Client.js",
		"./FieldOperators": "./dist/sdk/FieldOperators.js",
		"./Group": "./dist/sdk/Group.js",
		"./Module": "./dist/sdk/Module.js",
		"./Role": "./dist/sdk/Role.js",
		"./types": "./dist/sdk/types.js",
		"./types/metadata": "./dist/sdk/types/metadata.js",
		"./User": "./dist/sdk/User.js",
		"./TypeUtils": "./dist/sdk/TypeUtils.js",
		"./package.json": "./package.json"
	},
	"typesVersions": {
		"*": {
			"Client": [
				"./dist/sdk/Client.d.ts"
			],
			"FieldOperators": [
				"./dist/sdk/FieldOperators.d.ts"
			],
			"Group": [
				"./dist/sdk/Group.d.ts"
			],
			"Module": [
				"./dist/sdk/Module.d.ts"
			],
			"Role": [
				"./dist/sdk/Role.d.ts"
			],
			"types": [
				"./dist/sdk/types.d.ts"
			],
			"types/metadata": [
				"./dist/sdk/types/metadata.d.ts"
			],
			"User": [
				"./dist/sdk/User.d.ts"
			],
			"TypeUtils": [
				"./dist/sdk/TypeUtils.d.ts"
			]
		}
	},
	"publishConfig": {
		"access": "public"
	},
	"bin": {
		"konecty": "dist/cli/index.js"
	},
	"engines": {
		"node": ">=16.0.0",
		"yarn": ">=1.22.0",
		"npm": ">=7.10.0"
	},
	"scripts": {
		"clean": "rimraf dist *.tsbuildinfo",
		"start": "cross-env NODE_OPTIONS='--inspect' nodemon --watch 'src/' --exec 'ts-node src/index.ts' -e ts | pino-pretty --colorize --translateTime",
		"test": "jest --detectOpenHandles --runInBand",
		"test:watch": "jest --detectOpenHandles --runInBand --watch",
		"build": "yarn clean && yarn build:babel && yarn build:types",
		"build:babel": "babel src --out-dir dist --ignore '**/*.test.js' --ignore '**/__test__' --no-copy-ignored --extensions .ts",
		"build:types": "tsc -b tsconfig.build.json",
		"lint": "eslint src/**/*.ts --fix",
		"prettier": "prettier --write src",
		"prepare": "husky install"
	},
	"devDependencies": {
		"@babel/cli": "^7.17.6",
		"@babel/core": "^7.17.9",
		"@babel/node": "^7.16.8",
		"@babel/plugin-proposal-class-properties": "^7.17.12",
		"@babel/plugin-proposal-decorators": "^7.17.12",
		"@babel/plugin-proposal-private-methods": "^7.18.6",
		"@babel/preset-env": "^7.16.11",
		"@babel/preset-typescript": "^7.16.7",
		"@commitlint/cli": "^16.2.3",
		"@commitlint/config-conventional": "^16.2.1",
		"@semantic-release/changelog": "^6.0.1",
		"@semantic-release/git": "^10.0.1",
		"@tsconfig/node16": "^1.0.2",
		"@types/bluebird": "^3.5.36",
		"@types/chai": "^4.3.1",
		"@types/ini": "^1.3.31",
		"@types/inquirer": "^8.2.1",
		"@types/isomorphic-fetch": "^0.0.36",
		"@types/jest": "^27.4.1",
		"@types/js-cookie": "^3.0.2",
		"@types/lodash": "^4.14.182",
		"@types/mkdirp": "^1.0.2",
		"@types/node": "^17.0.27",
		"@types/numeral": "^2.0.2",
		"@types/prettier": "^2.6.1",
		"@types/qs": "^6.9.7",
		"@types/supertest": "^2.0.12",
		"@typescript-eslint/eslint-plugin": "^5.21.0",
		"@typescript-eslint/parser": "^5.21.0",
		"chai": "^4.3.6",
		"cross-env": "^7.0.3",
		"eslint": "^8.14.0",
		"husky": "^7.0.4",
		"jest": "27",
		"msw": "^0.49.2",
		"nodemon": "^2.0.15",
		"rimraf": "^3.0.2",
		"semantic-release": "^19.0.2",
		"supertest": "^6.2.2",
		"ts-jest": "^27.1.4",
		"ts-node": "^10.7.0",
		"typescript": "^4.6.3"
	},
	"dependencies": {
		"@types/luxon": "^2.3.2",
		"bluebird": "^3.7.2",
		"browser-or-node": "^2.1.1",
		"chalk": "4",
		"commander": "^9.2.0",
		"fastify": "^3.28.0",
		"fastify-plugin": "^3.0.1",
		"ini": "^3.0.0",
		"inquirer": "^8.2.4",
		"isomorphic-fetch": "^3.0.0",
		"js-cookie": "^3.0.1",
		"lodash": "^4.17.21",
		"luxon": "^2.4.0",
		"mkdirp": "^1.0.4",
		"numeral": "^2.0.6",
		"pino": "^7.10.0",
		"pino-pretty": "^7.6.1",
		"prettier": "^2.6.2",
		"qs": "^6.10.3",
		"reflect-metadata": "^0.1.13",
		"yaml": "^2.1.0"
	}
}
